defaults
port 587
protocol smtp
auth on
tls on
tls_trust_file /usr/local/etc/openssl/cert.pem

account default
host smtp.gmail.com
from rajesh@surf.net.np
user rajesh@surf.net.np
passwordeval get-password personalmail

account work
host outlook.office365.com
from <work-email>
user <work-email>
auth login
passwordeval get-password work
